\n            console.log(error.message);\n        }\n\n        window.location=\"/\";\n    }\n\n    const handleUpdate =(e,id)=>{\n        e.stopPropagation()\n        history.push(`/restaurants/${id}/update`);\n    }\n\n    const handleRestaurantSelect=(id)=>{\n        history.push(`/restaurants/${id}`)\n    }\n\n    const list =restaurants.map(restaurant=>(\n        <tr key={restaurant.id}  onClick={()=>handleRestaurantSelect(restaurant.id)}>\n            <td>{restaurant.name}</td>\n            <td>{restaurant.location}</td>\n            <td>{\"⭐\".repeat(restaurant.price_range)}</td>\n            <td>⭐⭐</td>\n            <td><button  className=\"btn btn-warning\" onClick={(e)=>handleUpdate(e,restaurant.id)}>Update</button></td>\n            <td><button  className=\"btn btn-danger\" onClick={(e)=>handleDelete(e,restaurant.id)}>Delete</button></td>\n        </tr>\n    ))\n    return (\n        <div>\n            <table className=\"table table-dark table-borderless table-hover\">\n                <thead>\n                    <tr  className=\"bg-primary\" >\n                        <th  scope=\"col\">Restaurants</th>\n                        <th  scope=\"col\">Location</th>\n                        <th  scope=\"col\">Price Range</th>\n                        <th  scope=\"col\">Ratings</th>\n                        <th  scope=\"col\">Edit</th>\n                        <th  scope=\"col\">delete</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {list}\n                </tbody>\n            </table>\n        </div>\n    )\n}\n\nexport default RestaurantList\n","/media/lugonzo/New Volume/Web projects/React/YELP APP/frontend/yelp-app/src/context/RestaurantContextAPI.js",[],"/media/lugonzo/New Volume/Web projects/React/YELP APP/frontend/yelp-app/src/components/UpdateRestaurant.js",["68","69"],"import React ,{useState,useContext,useEffect} from 'react'\nimport { useHistory, useParams } from 'react-router-dom'\nimport { R